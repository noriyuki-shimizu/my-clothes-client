{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --port 8888",
        "serve:open": "vue-cli-service serve --open --port 8888",
        "clean": "rimraf dist",
        "build": "vue-cli-service build",
        "deploy": "firebase deploy",
        "deploy:hosting": "firebase deploy --only hosting:mcm-client",
        "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
        "format": "prettier --write --ignore-path .gitignore *.{js,ts,json} \"{src,tests}/**/*.{js,ts,vue,json}\"",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "scanner": "sonar-scanner"
    },
    "dependencies": {
        "@sentry/browser": "5.25.0",
        "@sentry/integrations": "5.25.0",
        "@sentry/tracing": "5.25.0",
        "ant-design-vue": "1.6.4",
        "axios": "1.6.0",
        "chart.js": "2.9.4",
        "core-js": "^3.4.3",
        "firebase": "7.22.1",
        "js-cookie": "2.2.1",
        "moment": "^2.24.0",
        "register-service-worker": "1.7.1",
        "url": "^0.11.0",
        "uuid": "^3.3.3",
        "vue": "^2.6.10",
        "vue-chartjs": "^3.5.0",
        "vue-class-component": "^7.0.2",
        "vue-i18n": "8.18.2",
        "vue-property-decorator": "^8.3.0",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.2",
        "vuex-persistedstate": "^2.7.0",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@intlify/eslint-plugin-vue-i18n": "0.10.0",
        "@types/axios": "^0.14.0",
        "@types/chart.js": "^2.9.5",
        "@types/core-js": "^2.5.2",
        "@types/eslint": "^6.1.3",
        "@types/firebase": "^3.2.1",
        "@types/jest": "^24.0.19",
        "@types/js-cookie": "2.2.6",
        "@types/less": "^3.0.1",
        "@types/node": "^12.12.22",
        "@types/prettier": "^1.19.0",
        "@types/stylus": "^0.48.32",
        "@types/uuid": "^3.4.6",
        "@types/vue-i18n": "7.0.0",
        "@vue/cli-plugin-babel": "^4.1.0",
        "@vue/cli-plugin-e2e-cypress": "^4.1.0",
        "@vue/cli-plugin-eslint": "^4.1.0",
        "@vue/cli-plugin-pwa": "4.5.3",
        "@vue/cli-plugin-typescript": "4.5.13",
        "@vue/cli-plugin-unit-jest": "^4.1.0",
        "@vue/cli-service": "4.5.13",
        "@vue/eslint-config-airbnb": "5.0.2",
        "@vue/eslint-config-prettier": "^5.0.0",
        "@vue/eslint-config-typescript": "^4.0.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-plugin-import": "^1.13.0",
        "eslint": "5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "firebase-tools": "7.14.0",
        "less": "^2.7.2",
        "less-loader": "^5.0.0",
        "lint-staged": "^9.4.3",
        "prettier": "^1.19.1",
        "rimraf": "3.0.2",
        "sonar-scanner": "^3.1.0",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "typescript": "~3.5.3",
        "vue-template-compiler": "^2.6.10"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue,ts}": [
            "vue-cli-service lint",
            "git add"
        ]
    },
    "engines": {
        "node": ">= 13.11.0",
        "npm": ">= 6.13.7"
    }
}
